@model IEnumerable<DiaDiem>
<form method="post" class="star-rating" style="font-size: 13px;">

    @foreach (var item in Model)
    {
        <a class="form-check" style="text-align:left;" href="@Url.Action("FilterLoCation","Tour", new { idLocation = item.TenTinh})" >
            <label class="form-check-label" for="exampleCheck1">
                @item.TenTinh
            </label>
        </a>
    }


</form>
